#!/usr/bin/env bash
    
installRtl8812au() {
    rm -fr rtl8812au
    sudo apt-get install -y linux-headers-$(uname -r)
    sudo apt-get install -y realtek-rtl88xxau-dkms dkms
    git clone https://github.com/aircrack-ng/rtl8812au
    cd rtl8812au
    make
    sudo make install
    cd ../
    rm -fr rtl8812au
}

installRtl8812au
iwconfig
echo "Reboot"
